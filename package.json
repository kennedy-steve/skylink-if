{
    "name": "skylink-if",
    "version": "1.0.0",
    "author": "Nathaniel Louis Tisuela",
    "description": "Bringing Infinite Flight to Discord",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=16.6.0"
    },
    "type": "module",
    "exports": [
        "./dist/start.js",
        "./dist/app.js"
    ],
    "scripts": {
        "build": "prisma generate && tsc --project tsconfig.json",
        "db:push": "prisma db push --preview-feature",
        "migrate:reset": "prisma migrate reset",
        "migrate:create": "prisma migrate dev --create-only",
        "migrate:dev": "prisma migrate dev",
        "migrate:deploy": "prisma migrate deploy",
        "prisma:generate": "prisma generate",
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "register": "npm run build && node --enable-source-maps dist/start.js --register",
        "start": "npm run build && node --enable-source-maps dist/start.js",
        "start:shard": "npm run build && node --enable-source-maps dist/app.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "@prisma/client": "3.5.0",
        "dotenv": "^10.0.0",
        "prisma": "3.5.0",
        "@discordjs/rest": "0.2.0-canary.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "discord-api-types": "0.26.1",
        "discord.js": "13.6.0",
        "discord.js-rate-limiter": "1.3.1",
        "express": "4.17.3",
        "express-promise-router": "4.1.1",
        "filesize": "8.0.6",
        "linguini": "1.3.1",
        "luxon": "2.3.0",
        "node-fetch": "3.1.1",
        "node-schedule": "2.1.0",
        "pino": "7.6.3",
        "pino-pretty": "7.3.0",
        "pm2": "^5.1.2",
        "reflect-metadata": "^0.1.13",
        "remove-markdown": "0.3.0"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/luxon": "2.0.9",
        "@types/node": "17.0.9",
        "@types/node-fetch": "3.0.2",
        "@types/node-schedule": "1.3.2",
        "@types/remove-markdown": "0.3.1",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^8.7.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-unicorn": "^40.1.0",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    }
}
