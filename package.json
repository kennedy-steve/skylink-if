{
    "name": "skylink-if",
    "version": "1.0.0",
    "author": "Nathaniel Louis Tisuela",
    "description": "Bringing Infinite Flight to Discord",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=16.6.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "lint": "tslint --project .",
        "lint:fix": "tslint --fix --project .",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "build": "prisma generate && tsc --project tsconfig.json",
        "register": "npm run build && node --enable-source-maps dist/start.js --register",
        "start": "npm run build && node --enable-source-maps dist/start.js",
        "start:dev": "ts-node-dev --watch src/**/*.ts,config/**/*.json,lang/**/*.json src/start.ts",
        "start:shard": "npm run build && node --enable-source-maps dist/app.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json",
        "update:infinite-flight-data": "ts-node ./src/scripts/update-infinite-flight-data.ts",
        "db:push": "prisma db push --preview-feature",
        "migrate:reset": "prisma migrate reset",
        "migrate:create": "prisma migrate dev --create-only",
        "migrate:dev": "prisma migrate dev",
        "migrate:deploy": "prisma migrate deploy",
        "prisma:generate": "prisma generate"
    },
    "dependencies": {
        "@discordjs/rest": "0.1.0-canary.0",
        "@prisma/client": "3.5.0",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "discord-api-types": "0.24.0",
        "discord.js": "13.3.1",
        "discord.js-rate-limiter": "1.2.3",
        "dotenv": "^10.0.0",
        "express": "4.17.1",
        "express-promise-router": "4.1.0",
        "filesize": "8.0.6",
        "linguini": "1.2.9",
        "node-fetch": "2.6.5",
        "node-schedule": "2.0.0",
        "pino": "7.2.0",
        "pino-pretty": "7.2.0",
        "pm2": "^5.1.2",
        "prisma": "3.5.0",
        "reflect-metadata": "^0.1.13",
        "remove-markdown": "0.3.0",
        "ts-node": "^10.4.0"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/node": "^16.11.9",
        "@types/node-fetch": "2.5.12",
        "@types/node-schedule": "1.3.2",
        "@types/remove-markdown": "0.3.1",
        "prettier": "^2.4.1",
        "ts-node-dev": "^1.1.8",
        "tslint": "^6.1.3",
        "typescript": "^4.5.2"
    }
}